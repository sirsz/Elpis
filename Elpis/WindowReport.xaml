<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Elpis"
        xmlns:chartingToolkit="clr-namespace:System.Windows.Controls.DataVisualization.Charting;assembly=System.Windows.Controls.DataVisualization.Toolkit" x:Class="WindowReport3"
        mc:Ignorable="d"
        Title="SRIZ-S Raport" Height="430" Width="650" Icon="ELogo.ico">

    <Grid>
        <chartingToolkit:Chart Height="Auto" HorizontalAlignment="Stretch" 
                               Name="columnChart" Title="Wskaźnik ryzyk wizyt" 
                               Width="Auto" Margin="10">
            <chartingToolkit:ColumnSeries x:Name="columnSeries" DependentValuePath="Value" IndependentValuePath="Key" />
        </chartingToolkit:Chart>
    </Grid>
</Window>
